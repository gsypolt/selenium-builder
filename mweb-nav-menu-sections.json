{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://mhigh.indystar.com/"
    },
    {
      "type": "waitForElementPresent",
      "locator": {
        "type": "css selector",
        "value": ".menu-toggle-btn.main-nav-btn"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "a.menu-toggle-btn.main-nav-btn"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "NEWS"
      }
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "css selector",
        "value": "div.header-logo.header-news \n        \t\n        \t"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "a.menu-toggle-btn.main-nav-btn"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "SPORTS"
      }
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "css selector",
        "value": "div.header-logo.header-sports \n        \t\n        \t"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "a.menu-toggle-btn.main-nav-btn"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "LIFE"
      }
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "css selector",
        "value": "div.header-logo.header-life \n        \t\n        \t"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}